.. Math
.. Defines a set of math utility functions that can be used
program Math
const m := 7, n := "TEST";
var x,y,z,q,r : integer;

.. multiply
.. multiplies two numbers together
procedure multiply;
	var a,b : integer;
begin a := x; b := y; z := 0;
	while b > 0 do
	begin
		if odd b then z := z + a;
		if odd b then 
			if odd z then 
				z := z + b;
		a := 2*a; b := b/2;
	end
end ;
.
